{
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.startupEditor": "none",
  "editor.minimap.enabled": false,
  "terminal.integrated.defaultProfile.linux": "bash",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
  },
  "editor.editContext": false,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "terminal.integrated.sendKeybindingsToShell": true,
  "terminal.integrated.fontFamily": "Fira Mono, Consolas, 'Courier New', monospace",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "normal",
  "workbench.colorCustomizations": {
    "terminal.ansiBlack": "#693cff",
    "terminal.ansiRed": "#d12935",
    "terminal.ansiGreen": "#16e652",
    "terminal.ansiYellow": "#fff2e6",
    "terminal.ansiBlue": "#1876e6",
    "terminal.ansiMagenta": "#ef1759",
    "terminal.ansiCyan": "#26f4c6",
    "terminal.ansiWhite": "#d0cfcc",
    "terminal.ansiBrightBlack": "#5e5c64",
    "terminal.ansiBrightRed": "#ed2f1b",
    "terminal.ansiBrightGreen": "#31da78",
    "terminal.ansiBrightYellow": "#e2e90c",
    "terminal.ansiBrightBlue": "#2a7bde",
    "terminal.ansiBrightMagenta": "#d532ce",
    "terminal.ansiBrightCyan": "#2cbce9",
    "terminal.ansiBrightWhite": "#ffffff",
  },
  "googleGemini.enableAutocomplete": false,
  "googleGemini.inlineCompletion.enable": false,
  "googleGemini.enableSmartEdit": false,
  "editor.inlineSuggest.enabled": false,

  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["K"],
      "commands": ["editor.action.showHover"],
    },
    {
      "before": ["g", "d"],
      "commands": ["editor.action.revealDefinition"],
    },
    {
      "before": ["g", "r"],
      "commands": ["editor.action.referenceSearch.trigger"],
    },
  ],
  "editor.acceptSuggestionOnCommitCharacter": false,

  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
  },
  "[dockercompose]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "advanced",
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true,
    },
    "editor.defaultFormatter": "redhat.vscode-yaml",
  },
  "[github-actions-workflow]": {
    "editor.defaultFormatter": "redhat.vscode-yaml",
  },
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command",
    "switch:command",
  ],
  "remote.autoForwardPortsSource": "hybrid",
  "notebook.lineNumbers": "on",
  "jupyter.interactiveWindow.creationMode": "perFile",
  "geminicodeassist.project": "emerald-fire-7vrh3",
  "geminicodeassist.coreTools": ["ShellTool"],
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "gitlens.views.scm.grouped.views": {
    "commits": true,
    "branches": true,
    "remotes": true,
    "stashes": true,
    "tags": true,
    "worktrees": true,
    "contributors": true,
    "fileHistory": true,
    "repositories": true,
    "searchAndCompare": false,
    "launchpad": true,
  },
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path-linux": "/home/kougami/.local/state/vs-kubernetes/tools/helm/linux-amd64/helm",
  },
  "settingsSync.ignoredExtensions": [],
  "markdown-pdf.mermaidServer": "https://unpkg.com/mermaid@10.4.0/dist/mermaid.js",
  "workbench.editorAssociations": {
    "*.copilotmd": "vscode.markdown.preview.editor",
    "*.db": "default",
    "*.png": "imagePreview.previewEditor",
  },
  "workbench.editor.enablePreview": false,
  "workbench.imagePreview.interpolation": "off",
  "geminicodeassist.displayInlineContextHint": false,
  "latex-workshop.latex.autoBuild.run": "onSave",
  "latex-workshop.latex.recipe.default": "ptex2pdf",
  "latex-workshop.latex.tools": [
    {
      "name": "ptex2pdf",
      "command": "ptex2pdf",
      "args": [
        "-l",
        "-ot",
        "-kanji=utf8 -synctex=1 -file-line-error -interaction=nonstopmode", // ここに追記
        "%DOC%",
      ],
    },
  ],
  "latex-workshop.latex.recipes": [
    {
      "name": "ptex2pdf",
      "tools": ["ptex2pdf"],
    },
  ],
  "markdown.extension.toc.updateOnSave": false,
  "workbench.preferredHighContrastColorTheme": "GitHub Dark Colorblind (Beta)",
  "editor.unicodeHighlight.invisibleCharacters": false,
  "todohighlight.isEnable": true,
  "todohighlight.keywords": [
    {
      "text": "，",
      "border": "1px solid #ffff00",
      "backgroundColor": "rgba(255, 255, 0, 0.4)",
      "isWholeLine": false,
    },
    {
      "text": "．",
      "border": "1px solid #ffff00",
      "backgroundColor": "rgba(255, 255, 0, 0.4)",
      "isWholeLine": false,
    },
    {
      "text": "）",
      "border": "1px solid #ffff00",
      "backgroundColor": "rgba(255, 255, 0, 0.4)",
      "isWholeLine": false,
    },
    {
      "text": "（",
      "border": "1px solid #ffff00",
      "backgroundColor": "rgba(255, 255, 0, 0.4)",
      "isWholeLine": false,
    },
    {
      "text": "(",
      "border": "1px solid #f00",
      "backgroundColor": "rgba(255, 0, 0, 0.4)",
      "isWholeLine": false,
    },
    {
      "text": ")",
      "border": "1px solid #f00",
      "backgroundColor": "rgba(255, 0, 0.4)",
      "isWholeLine": false,
    },
    {
      "text": ",",
      "border": "1px solid #f00",
      "backgroundColor": "rgba(255, 0, 0, 0.4)",
      "isWholeLine": false,
    },
    {
      "text": ".",
      "border": "1px solid #f00",
      "backgroundColor": "rgba(255, 0, 0.4)",
      "isWholeLine": false,
    },
  ],
  "todohighlight.include": ["**/*.tex"],
}
